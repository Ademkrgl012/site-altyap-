<title>Fynx Code | {{member.user.username}}</title> <br><br> <div class="container">
  <div class="twPc-div bg-dark">
    <a class="twPc-bg twPc-block"></a>
    <div>
      <div class="twPc-button">
        <a href="https://discordapp.com/users/{{member.user.id}}" class="btn btn-primary btn-sm">Discord</a>  
      </div>
      <a href="https://discordapp.com/users/{{member.user.id}}" class="twPc-avatarLink">
        <img alt="Avatar" src="{{avatarURL}}" class="twPc-avatarImg">
      </a>
      <div class="twPc-divUser">
        <div class="twPc-divName">
          {{member.user.username}}
        </div>
        <span>
          {{#if perms.yetkili}}
            <span>Kod Paylaşım Yetkilisi</span>
          {{else}}
            {{#if perms.destekçi}}
              <span>Fynx Code Destekçisi</span>
            {{else}}
              <span>Kullanıcı</span>
            {{/if}}
          {{/if}}
        </span>
        {{#if stats}}
          <span> | Paylaşılan: {{stats.paylasilan}} | Silinen: {{stats.silinen}} </span>
        {{/if}}        
      </div>
      <div class="twPc-divStats">
        <h3>Görebildiği Kodlar</h3>
        <ul class="twPc-Arrange">
          <a href="/normal" style="text-decoration:none" class="index-button1"><i class="fas fa-code"></i> Normal Kodlar</a>
          {{#if perms.yetkili}}
       <a href="/altin"  style="text-decoration:none" class="index-button2"><i class="fas fa-code"></i> Altın Kodlar</a>
       <a href="/elmas" style="text-decoration:none" class="index-button3"><i class="fas fa-code"></i> Elmas Kodlar</a>
       <a href="/hazir" style="text-decoration:none" class="index-button4"><i class="fas fa-code"></i> Hazır Sistemler</a>
          {{else}}
            {{#if perms.destekçi}}
            <a href="/altin"  style="text-decoration:none" class="index-button2"><i class="fas fa-code"></i> Altın Kodlar</a>
            <a href="/elmas" style="text-decoration:none" class="index-button3"><i class="fas fa-code"></i> Elmas Kodlar</a>
            <a href="/hazir" style="text-decoration:none" class="index-button4"><i class="fas fa-code"></i> Hazır Sistemler</a>
            {{else}}
              {{#if perms.altin}}
            <a href="/altin"  style="text-decoration:none" class="index-button2"><i class="fas fa-code"></i> Altın Kodlar</a>
              {{/if}}
              {{#if perms.elmas}}
            <a href="/elmas" style="text-decoration:none" class="index-button3"><i class="fas fa-code"></i> Elmas Kodlar</a>
              {{/if}}
              {{#if perms.hazir}}
           <a href="/hazir" style="text-decoration:none" class="index-button4"><i class="fas fa-code"></i> Hazır Sistemler</a>
              {{/if}}
            {{/if}}
          {{/if}}  
        </ul>
      </div>
    </div>
  </div>
</div>

